{% extends "base_template.twig" %}

{% block content %}

    <h3>User Groups</h3>
    <a href="#">Join group</a>
    <div>
        <ul>
            {% for group in groups %}
                <li>{{ group.name }}<=#|#=>> {{ group.joined_date }} <=#|#=>> <a href="#">Edit</a> <=#|#=>> <a href="#">Delete</a>
                </li>
            {% else %}
                <li><em>no user found</em></li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}